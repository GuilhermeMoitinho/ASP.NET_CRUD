@model DIVTECH2.Models.FornecedorModel
@{
    ViewData["Title"] = "Detalhes do seu Fornecedor";
}

<h1>Detalhes</h1>

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(x => x.Nome)
        </dt>
        <dt class="col-sm-10">
            @Html.DisplayFor(x => x.Nome)
        </dt>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(x => x.CNPJ)
        </dt>
        <dt class="col-sm-10">
            @Html.DisplayFor(x => x.CNPJ)
        </dt>
        
        <dt class="col-sm-2">
            @Html.DisplayNameFor(x => x.Especialidade)
        </dt>
        <dt class="col-sm-10">
            @Html.DisplayFor(x => x.Especialidade)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(x => x.CEP)
        </dt>
        <dt class="col-sm-10">
            @Html.DisplayFor(x => x.CEP)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(x => x.Endereco)
        </dt>
        <dt class="col-sm-10">
            @Html.DisplayFor(x => x.Endereco)
        </dt>
    </dl>
</div>
<a asp-action="Editar" asp-route-id="@Model.Id" class="btn btn-warning">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Voltar</a>